{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/_components/*"
      ],
      "#/*": [
        "src/layouts/*"
      ]
      ,
      "$/*": [
        "src/stores/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}